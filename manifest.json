{
  "name": "reverse audio",
  "version": "0.0.1",
  "author": "nulla",
  "manifest_version": 3,
  "description": "record and reverse web audio",
  "action": {
    "default_popup": "popup.html",
    "default_title": "record"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "assets/icon.png"
  },
  "permissions": [
    "activeTab",
    "tabCapture"
  ],
  "commands": {
    "toggle": {
      "description": "Toggle Record",
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      }
    }
  }
}